{
  "name": "MySpass Adblocker",
  "short_name": "MySpass Adblocker",
  "version": "0.0.12",
  "manifest_version": 2,
  "description": "Diese Erweiterung blockiert die l√§stigen Werbeeinblendungen auf Myspass.de (blockiert auch die adblock Warnung).",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "de",
  "background": {
    "scripts": [
	  "js/libs/jquery/jquery.min.js",
      "js/bg/background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "MySpass Adblocker",
    "default_popup": "page_action/page_action.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.myspass.de/*"
      ],
      "js": [
		"js/libs/2013_misc.js",
		"js/libs/swfobject.js",
		"js/libs/player.js",

	    "js/libs/jquery/jquery.min.js",
        "js/inject/inject.js"
      ]
    }
  ]
}